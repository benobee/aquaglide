{
    "name": "sqs-build",
    "version": "1.0.0",
    "description": "",
    "main": "main.js",
    "scripts": {
        "test": "mocha",
        "start": "webpack --progress --colors --watch --mode=development&&documentation build --output ./README.md --format md",
        "watch": "./node_modules/.bin/webpack -d --watch --colors",
        "build": "set PROD_ENV=true && set process.env='production' && webpack -p --mode=production&&documentation build --output ./README.md --format md",
        "dev": "webpack-dev-server ./main.js --hot --watch --colors --mode=development&&documentation build --output ./README.md --format md",
        "sqs": "squarespace-server https://aquaglide.squarespace.com --auth -d ./template"
    },
    "babel": {
        "presets": [
            "env"
        ]
    },
    "author": "Ben Obee",
    "license": "ISC",
